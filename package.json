    "eslint-plugin-import": "^2.2.0",
    "ghooks": "^1.2.4",
    "istanbul": "^0.4.4",
    "nodeunit": "^0.10.2",
    "redis": "^2.6.2",
    "semantic-release": "^6.3.2",
